@model Langben.DAL.EmployeeMiddleImportRecord

@{
    Layout = "~/Views/Shared/Details.cshtml";
}
@using Common
 @section CurentPlace {
    详细  
}
 
    <fieldset>
        <legend>
            <input class="a2 f2" type="button" onclick="window.location.href = 'javascript:history.go(-1)';" value="返回" />
        </legend>
        <div class="bigdiv">
                  
                <div class="display-label">
                      @Html.LabelFor(model => model.URL) :
                </div>
                <div class="display-field" id='URL'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.ImportCount) :
                </div>
                <div class="display-field" id='ImportCount'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.ImportPayment) :
                </div>
                <div class="display-field" id='ImportPayment'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.CreateTime) :
                </div>
                <div class="display-field" id='CreateTime'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.CreateUserID) :
                </div>
                <div class="display-field" id='CreateUserID'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.CreateUserName) :
                </div>
                <div class="display-field" id='CreateUserName'>
                </div>
        </div>
    </fieldset>

<script type="text/javascript">

    $(function () {
        // 获取内容并显示
        var id = '@ViewBag.Id';
        $.ajax({
            url: "/api/EmployeeMiddleImportRecordApi/" + id,
            type: "POST",
            // async: false,
            success: function (res) {
		$("#URL").html(res.URL);$("#ImportCount").html(res.ImportCount);$("#ImportPayment").html(res.ImportPayment);$("#CreateTime").html(res.CreateTime);$("#CreateUserID").html(res.CreateUserID);$("#CreateUserName").html(res.CreateUserName);
            }
        })
    });
</script>

