@model Langben.DAL.CompanyEmployeeRelation

@{
    Layout = "~/Views/Shared/Details.cshtml";
}
@using Common
 @section CurentPlace {
    详细  
}
 
    <fieldset>
        <legend>
            <input class="a2 f2" type="button" onclick="window.location.href = 'javascript:history.go(-1)';" value="返回" />
        </legend>
        <div class="bigdiv">
                  
                <div class="display-label">
                      @Html.LabelFor(model => model.EmployeeId) :
                </div>
                <div class="display-field" id='EmployeeId'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.CompanyId) :
                </div>
                <div class="display-field" id='CompanyId'>
                </div>
                <br style="clear: both;" />
                <div class="display-label">
                    @Html.LabelFor(model => model.Remark) :
                </div>
                <div class="textarea-box">
                    @Html.TextAreaFor(model => model.Remark, new {  @readonly=true})                   
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.State) :
                </div>
                <div class="display-field" id='State'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.CreateTime) :
                </div>
                <div class="display-field" id='CreateTime'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.CreatePerson) :
                </div>
                <div class="display-field" id='CreatePerson'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.UpdateTime) :
                </div>
                <div class="display-field" id='UpdateTime'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.UpdatePerson) :
                </div>
                <div class="display-field" id='UpdatePerson'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.CityId) :
                </div>
                <div class="display-field" id='CityId'>
                </div>
        </div>
    </fieldset>

<script type="text/javascript">

    $(function () {
        // 获取内容并显示
        var id = '@ViewBag.Id';
        $.ajax({
            url: "/api/CompanyEmployeeRelationApi/" + id,
            type: "POST",
            // async: false,
            success: function (res) {
		$("#EmployeeId").html(res.EmployeeId);$("#CompanyId").html(res.CompanyId);$("#Remark").val(res.Remark);$("#State").html(res.State);$("#CreateTime").html(res.CreateTime);$("#CreatePerson").html(res.CreatePerson);$("#UpdateTime").html(res.UpdateTime);$("#UpdatePerson").html(res.UpdatePerson);$("#CityId").html(res.CityId);
            }
        })
    });
</script>

