 }
@using (Html.BeginForm("", "api/CompanyEmployeeRelationApi/Post"))
{
    <fieldset>
        <legend>
            <input class="a2 f2" type="submit" value="创建" />
            <input class="a2 f2" type="button" onclick="BackList('CompanyEmployeeRelation')" value="返回" />
        </legend>
        <div class="bigdiv">
                 
            <div class="editor-label">
                @Html.LabelFor(model => model.EmployeeId) ：
            </div>
            <div class="editor-field">
                @Html.TextBoxFor(model => model.EmployeeId, new {  onkeyup = "isInt(this)", @class="text-box single-line" })
