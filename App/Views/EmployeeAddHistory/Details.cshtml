<script type="text/javascript">

    $(function () {
        // 获取内容并显示
        var id = '@ViewBag.Id';
        $.ajax({
            url: "/api/EmployeeAddHistoryApi/" + id,
            type: "POST",
            // async: false,
            success: function (res) {
		$("#Wage").html(res.Wage);$("#CompanyEmployeeRelationId").html(res.CompanyEmployeeRelationId);$("#PoliceInsuranceId").html(res.PoliceInsuranceId);$("#InsuranceCode").html(res.InsuranceCode);$("#PoliceOperationId").html(res.PoliceOperationId);$("#InsuranceMonth").html(res.InsuranceMonth);$("#StartTime").html(res.StartTime);$("#PoliceAccountNatureId").html(res.PoliceAccountNatureId);$("#SuppliersId").html(res.SuppliersId);$("#IsIndependentAccount").html(res.IsIndependentAccount);$("#Remark").val(res.Remark);$("#State").html(res.State);$("#CreateTime").html(res.CreateTime);$("#CreatePerson").html(res.CreatePerson);$("#UpdateTime").html(res.UpdateTime);$("#UpdatePerson").html(res.UpdatePerson);
            }
        })
    });
</script>

