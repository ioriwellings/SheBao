@model Langben.DAL.PRD_Product

@{
    Layout = "~/Views/Shared/Details.cshtml";
}
@using Common
 @section CurentPlace {
    详细  
}
 
    <fieldset>
        <legend>
            <input class="a2 f2" type="button" onclick="window.location.href = 'javascript:history.go(-1)';" value="返回" />
        </legend>
        <div class="bigdiv">
                  
                <div class="display-label">
                      @Html.LabelFor(model => model.ProductName) :
                </div>
                <div class="display-field" id='ProductName'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.Status) :
                </div>
                <div class="display-field" id='Status'>
                </div>
        </div>
    </fieldset>

<script type="text/javascript">

    $(function () {
        // 获取内容并显示
        var id = '@ViewBag.Id';
        $.ajax({
            url: "/api/PRD_ProductApi/" + id,
            type: "POST",
            // async: false,
            success: function (res) {
		$("#ProductName").html(res.ProductName);$("#Status").html(res.Status);
            }
        })
    });
</script>

