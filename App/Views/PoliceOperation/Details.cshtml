@model Langben.DAL.PoliceOperation

@{
    Layout = "~/Views/Shared/Details.cshtml";
}
@using Common
 @section CurentPlace {
    详细  
}
 
    <fieldset>
        <legend>
            <input class="a2 f2" type="button" onclick="window.location.href = 'javascript:history.go(-1)';" value="返回" />
        </legend>
        <div class="bigdiv">
                  
                <div class="display-label">
                      @Html.LabelFor(model => model.Name) :
                </div>
                <div class="display-field" id='Name'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.Style) :
                </div>
                <div class="display-field" id='Style'>
                </div>
                <br style="clear: both;" />
                <div class="display-label">
                    @Html.LabelFor(model => model.Remark) :
                </div>
                <div class="textarea-box">
                    @Html.TextAreaFor(model => model.Remark, new {  @readonly=true})                   
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.State) :
                </div>
                <div class="display-field" id='State'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.CreateTime) :
                </div>
                <div class="display-field" id='CreateTime'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.CreatePerson) :
                </div>
                <div class="display-field" id='CreatePerson'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.UpdateTime) :
                </div>
                <div class="display-field" id='UpdateTime'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.UpdatePerson) :
                </div>
                <div class="display-field" id='UpdatePerson'>
                </div>    
                <div class="display-label">
                      @Html.LabelFor(model => model.InsuranceKindId) :
                </div>
                <div class="display-field">            
                    @{string ids10 = string.Empty;
                       foreach (var item10 in Model.InsuranceKind)
                       {
                           ids10 += item10.Name;
                           ids10 += " , ";
                                 
                     } 
                    
                @ids10   
                  }
                </div>
        </div>
    </fieldset>

<script type="text/javascript">

    $(function () {
        // 获取内容并显示
        var id = '@ViewBag.Id';
        $.ajax({
            url: "/api/PoliceOperationApi/" + id,
            type: "POST",
            // async: false,
            success: function (res) {
		$("#Name").html(res.Name);$("#Style").html(res.Style);$("#Remark").val(res.Remark);$("#State").html(res.State);$("#CreateTime").html(res.CreateTime);$("#CreatePerson").html(res.CreatePerson);$("#UpdateTime").html(res.UpdateTime);$("#UpdatePerson").html(res.UpdatePerson);
            }
        })
    });
</script>

