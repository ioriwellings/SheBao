@model Langben.DAL.COST_ExpenditureInsurance

@{
    Layout = "~/Views/Shared/Details.cshtml";
}
@using Common
 @section CurentPlace {
    详细  
}
 
    <fieldset>
        <legend>
            <input class="a2 f2" type="button" onclick="window.location.href = 'javascript:history.go(-1)';" value="返回" />
        </legend>
        <div class="bigdiv">
                  
                <div class="display-label">
                      @Html.LabelFor(model => model.Status) :
                </div>
                <div class="display-field" id='Status'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.CRM_Company_ID) :
                </div>
                <div class="display-field" id='CRM_Company_ID'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.CreateTime) :
                </div>
                <div class="display-field" id='CreateTime'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.CreateUserID) :
                </div>
                <div class="display-field" id='CreateUserID'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.CreateUserName) :
                </div>
                <div class="display-field" id='CreateUserName'>
                </div>      
                <div class="display-label">
                      @Html.LabelFor(model => model.BranchID) :
                </div>
                <div class="display-field" id='BranchID'>
                </div>
        </div>
    </fieldset>

<script type="text/javascript">

    $(function () {
        // 获取内容并显示
        var id = '@ViewBag.Id';
        $.ajax({
            url: "/api/COST_ExpenditureInsuranceApi/" + id,
            type: "POST",
            // async: false,
            success: function (res) {
		$("#Status").html(res.Status);$("#CRM_Company_ID").html(res.CRM_Company_ID);$("#CreateTime").html(res.CreateTime);$("#CreateUserID").html(res.CreateUserID);$("#CreateUserName").html(res.CreateUserName);$("#BranchID").html(res.BranchID);
            }
        })
    });
</script>

